<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd" 
catalog="" schema="${mainschema}" table="customers" datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
	<DeleteKeys>
		<Column>cst_id</Column>
	</DeleteKeys>
	<UpdateKeys>
		<Column>cst_id</Column>
	</UpdateKeys>
	<GeneratedKey>cst_id</GeneratedKey>	
	<Queries>
		<Query id="queryAgreementEmails">
			<Sentence>
				<![CDATA[
					SELECT
					#COLUMNS#
					FROM
					customers
					WHERE 
					cst_agree_spam = 1 
					AND cst_canceled is null	
					#WHERE_CONCAT#						     					     
				]]>
			</Sentence>
		</Query>
		<Query id="queryBasic">
			<Sentence>
				<![CDATA[
					SELECT
					#COLUMNS#
					FROM
					customers
					WHERE 
					cst_canceled is null	
					#WHERE_CONCAT#						     					     
				]]>
			</Sentence>
		</Query>
