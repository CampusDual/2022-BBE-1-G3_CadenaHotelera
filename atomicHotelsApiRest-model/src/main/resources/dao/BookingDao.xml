<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd" 
catalog="" schema="${mainschema}" table="bookings" datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
	<DeleteKeys>		
		<Column>bkg_id</Column>
	</DeleteKeys>
	<UpdateKeys>		
		<Column>bkg_id</Column>
	</UpdateKeys>
	<GeneratedKey>bkg_id</GeneratedKey>
	<Queries>
		<Query id="queryBookingsInRange">
			<Sentence>
				<![CDATA[
				    SELECT
				     	#COLUMNS#
				    FROM
				     	bookings				    
				    WHERE
				    	(range_checkin >= bkg_checkin AND range_chekout <= bkg_checkout)
				    	OR (range_checkin >= bkg_checkin AND range_chekin < bkg_checkout)
				    	OR (range_checkout <= bkg_checkout AND range_chekout > bkg_checkin)				    	   
				    #WHERE_CONCAT#				    
			    ]]>
			</Sentence>
		</Query>
	</Queries>			
</JdbcEntitySetup>