<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd" 
catalog="" schema="${mainschema}" table="receipts" datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
	<DeleteKeys>
		<Column>rcp_id</Column>
	</DeleteKeys>
	<UpdateKeys>
		<Column>rcp_id</Column>
	</UpdateKeys>
	<GeneratedKey>rcp_id</GeneratedKey>	
	 <Queries>
  <Query id="queryRecibo">
   <Sentence>
   <![CDATA[
     SELECT bkg_id, rmt_price, (bkg_end - bkg_start) AS rcp_dias 
     FROM PUBLIC.receipts LEFT JOIN public.bookings  ON rcp_bkg_id = bkg_id 
     LEFT JOIN public.rooms ON bkg_rm_id = rm_id 
     LEFT JOIN public.room_types ON rmt_id = rm_rmt_id
     #WHERE#
   ]]>
   </Sentence>
  </Query>
 </Queries>
</JdbcEntitySetup>